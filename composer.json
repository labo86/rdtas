{
  "name": "labo86/rdtas",
  "description": "Una biblioteca con utilidades",
  "authors": [
    {
      "name": "Edwin Rodríguez-León",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "suggest": {
    "labo86/staty" : "Para usar las rueditas parar staty",
    "labo86/hapi" : "Para usar las rueditas para hapi"
  },
  "require": {
    "php": "^7.4",
    "labo86/exception_with_data" : "^3.0",
    "ext-json": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mikey179/vfsstream": "^1.6",
    "labo86/staty" : "^1.0",
    "labo86/hapi" : "^4.1"
  },
  "autoload": {
    "psr-4": {
      "labo86\\rdtas\\": "src/"
    },
    "exclude-from-classmap": ["/src/testing/"]
  },
  "autoload-dev": {
    "psr-4": {
      "test\\labo86\\rdtas\\": "test/",
      "labo86\\rdtas\\testing\\" :"src/testing/"
    }
  },
  "archive": {
    "exclude": [
      "/.git",
      "/.idea",
      "/examples",
      "/scripts",
      "/test",
      "/.gitignore",
      "/.phpunit.result.cache",
      "/phpunit.xml.dist",
      "/README.md",
      "/.travis.yml"
    ]
  }
}
